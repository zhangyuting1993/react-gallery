{
  "name": "react-gallery",
  "version": "1.0.0",
  "description": "a gallery demo by react",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "test": "make test",
    "start": "npm run dev:local",
    "dev:local": "cross-env NODE_ENV=development node build/webpack.dev.js --hot",
    "dev:test": "cross-env NODE_ENV=development node build/webpack.dev.js --hot --deploy",
    "release": "npm run deploy:online",
    "deploy:test": "cross-env NODE_ENV=production node build/webpack.release.js --watch --uglify --deploy=test",
    "deploy:online": "cross-env NODE_ENV=production node build/webpack.release.js --uglify --deploy=online"
  },
  "keywords": [
    "react",
    "demo"
  ],
  "author": "zhangchuachua",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "cross-env": "^2.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.11.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.2.2",
    "file-loader": "^0.11.1",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.3.0",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.3.3",
    "precss": "^1.4.0",
    "press": "0.0.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  }
}
